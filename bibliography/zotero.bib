  volume = {20},
  number = {3},
  pages = {034021},
  publisher = {American Physical Society},
  doi = {10.1103/PhysRevApplied.20.034021},
  abstract = {The connectivity of self-assembled networks of nanowires and nanoparticles is believed to strongly influence their performance in brainlike (neuromorphic) computing applications. Here we present a new perspective on the connectivity of these networks in which their neuronlike active elements are viewed in the same way as the nodes in artificial and biological neuronal networks. We consider two-dimensional and quasi-three-dimensional networks of nanowires and percolating networks of nanoparticles and show that, from this new perspective, they all have similar small-world characteristics. Other characteristics which may impact the computational performance of the networks are also investigated, including their assortativity and the scalefree nature of the nanoparticle networks. Taken together, these results allow comparison of key network characteristics for a variety of self-assembled nanoscale networks, and provide a basis for detailed investigations of computational performance.},
  groups = {Paper},
  timestamp = {2025-06-30T07:34:29Z},
  file = {Full Text PDF:/Users/lucashirschfeld/Zotero/storage/7F4L9HYV/Daniels et al. - 2023 - Brainlike Networks of Nanowires and Nanoparticles A Change of Perspective.pdf:application/pdf;APS Snapshot:/Users/lucashirschfeld/Zotero/storage/IC2M8Y8Y/PhysRevApplied.20.html:text/html}
}

