  shorttitle = {The {{XPS}} of Pyridine},
  author = {Bagus, Paul S. and Nelin, Connie J. and Sassi, Michel and Baranowski, Daniel and Sharp, Marcus A. and Autrey, Tom and Dohnálek, Zdenek and Novotny, Zbynek},
