@article{Jia2019,
  title = {Nanowire {{Electronics}}: {{From Nanoscale}} to {{Macroscale}}},
  shorttitle = {Nanowire {{Electronics}}},
  author = {Jia, Chuancheng and Lin, Zhaoyang and Huang, Yu and Duan, Xiangfeng},
