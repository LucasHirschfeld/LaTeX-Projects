@article{Ruano2021,
  title = {Deprotonation of the Amine Group of {{Glyphosate}} Studied by {{XPS}} and {{DFT}}},
  author = {Ruano, G. and Pedano, M.L. and Albornoz, M. and Fuhr, J.D. and Martiarena, M.L. and Zampieri, G.},
  date = {2021-11},
  journaltitle = {Applied Surface Science},
  shortjournal = {Applied Surface Science},
  volume = {567},
  pages = {150753},
  issn = {01694332},
  doi = {10.1016/j.apsusc.2021.150753},
  abstract = {Glyphosate (N-(phosphonomethyl)glycine, C3H8NO5P) in aqueous solutions undergoes three deprotonation reactions generally assigned to the carboxylate oxygen (pKa1 \textasciitilde{} 2.2), the phosphonate oxygen (pKa2 \textasciitilde{} 5.5), and the amine group (pKa3 \textasciitilde{} 10.2). A couple of recent studies (J. Phys. Chem. A 2015, 119, 5241–5249 and J. Phys. Chem. B 2016, 120, 2132–2137), however, have put forward a new sequence in which the amine group deprotonates before the phosphonate oxygen. In this study we present experimental and theoretical data with the aim of resolving the controversy over the order of the last two deprotonations. On the experimental side, we analysed the evolution of the N1s photoemission spectrum with the pH of the aqueous solution, finding conclusive evidence that the last deprotonation occurs at the amine group. On the theoretical side, we compared the stabilities of several configurations for each glyphosate moiety, finding again that the last group that undergoes deprotonation is the amine group.},
  langid = {english},
  groups = {Paper},
  timestamp = {2025-06-26T14:41:10Z},
  file = {PDF:/Users/lucashirschfeld/Zotero/storage/NZZP7ILV/Ruano et al. - 2021 - Deprotonation of the amine group of Glyphosate studied by XPS and DFT.pdf:application/pdf}
}

